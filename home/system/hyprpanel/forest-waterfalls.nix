    enable = true;
    hyprland.enable = true;
    overlay.enable = true;
     layout = {
      "bar.layouts" = {
        "0" = {
          "left" = [ "dashboard" "workspaces" "windowtitle" ];
          "middle" = [ "media" ];
          "right" = [
            "volume"
            "network"
            "bluetooth"
            # "battery" # CHANGEME
            "systray"
            "clock"
            "notifications"
          ];
        };
      };
